    <servlet-mapping>
        <servlet-name>MagicSquare</servlet-name>
        <url-pattern>/MagicSquare</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Kostka</servlet-name>
        <url-pattern>/Kostka</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Iloczyn</servlet-name>
