        <servlet-name>Tabliczka</servlet-name>
        <url-pattern>/Tabliczka</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Walidacja</servlet-name>
        <url-pattern>/plk.do</url-pattern>
