    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Wykres</servlet-name>
        <url-pattern>/Wykres</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
