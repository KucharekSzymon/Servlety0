        <servlet-name>Walidacja</servlet-name>
        <servlet-class>com.mycompany.servlety0.Walidacja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MagicSquare</servlet-name>
        <servlet-class>com.mycompany.servlety0.MagicSquare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Kostka</servlet-name>
        <servlet-class>com.mycompany.servlety0.Kostka</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Iloczyn</servlet-name>
        <servlet-class>com.mycompany.servlety0.Iloczyn</servlet-class>
